generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = env("DATABASE_PROVIDER")
  url      = env("DATABASE_URL")
}

model endpoints {
  id           String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  method       String
  requestUrl   String @unique
  responseBody Json
  responseCode Int
}
